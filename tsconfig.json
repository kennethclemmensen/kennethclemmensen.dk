{
  "compilerOptions": {
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "incremental": true,
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noImplicitThis": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./public/wp-content/themes/kennethclemmensen/js/compiled",
    "sourceMap": false,
    "strict": true,
    "target": "es2020",
    "tsBuildInfoFile": "./tsc/.tsbuildinfo",    
    "types": [
      "jquery"
    ]
  },
  "include": [
    "./public/wp-content/themes/kennethclemmensen/ts/**/*.ts"
  ]
}